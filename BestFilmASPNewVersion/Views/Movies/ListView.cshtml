@using BestFilmASPNewVersion.Helpers
@model PageInfoModel



@{
    ViewData["Title"] = "Фильмы";
}
<h1> @ViewData["Title"] </h1>

@foreach (var movie in Model.Movies)
{
    @Html.Partial("MovieView", movie);
}

<nav aria-label="Page navigation">
    <ul page-model="Model" page-action="ListView"></ul>
</nav>
















